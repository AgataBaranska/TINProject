<header>
    <h1 id="shop-name">
      SeeMore</h1>
    <div id="weather">Weather info</div>
</header>

  <script>
    function fetchWeather(){
      fetch('http://api.weatherstack.com/current?access_key=18b357e2131320fa0a33f8c3ea9d55d5&query=New%20York')
      .then(response=>response.json())
      .then(json=>{
        let output = '<h2>Weather for New York</h2>';
        output+=json;
   

        document.getElementById('weather').innerHTML= output;
      })
    }
    fetchWeather();
  

  </script>